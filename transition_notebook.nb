Cell[TextData[{
 "The Zeeman effect is the shift of an energy level in an atom perturbed by a \
magnetic field. The energy offset for an energy eigenstate is \
\[CapitalDelta]E = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", 
     SubscriptBox["\[Mu]", "B"]}], 
    SubscriptBox["g", "j"], 
    SubscriptBox["m", "j"], "B"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Mu]", "B"], " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "is the  Bohr magneton and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["g", "j"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is the lande\[CloseCurlyQuote] g-factor given by, approximating the \
electron g-factor by 2,"
}], "Text",
 CellChangeTimes->{{3.7630672241150723`*^9, 3.763067294614106*^9}, {
  3.7630675143856835`*^9, 3.7630675939912395`*^9}, {3.76306774619895*^9, 
  3.7630677693352737`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"gLande", "[", 
    RowBox[{"S_", ",", "L_", ",", "J_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["3", "2"], "  ", "+", " ", 
    FractionBox[
     RowBox[{
      RowBox[{"S", 
       RowBox[{"(", 
        RowBox[{"S", "+", "1"}], ")"}]}], "-", 
      RowBox[{"L", 
       RowBox[{"(", 
        RowBox[{"L", "+", "1"}], ")"}]}]}], 
     RowBox[{"2", " ", "J", 
      RowBox[{"(", 
       RowBox[{"J", "+", "1"}], ")"}]}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalDelta]Uz", "[", 
   RowBox[{"B_", ",", "state_"}], "]"}], ":=", " ", 
  RowBox[{"-", 
   FractionBox[
    RowBox[{"\[Mu]B", " ", "B", " ", 
     RowBox[{"state", "[", 
      RowBox[{"[", "4", "]"}], "]"}], " ", 
     RowBox[{"gLande", "[", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"state", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "-", "1"}], "2"], ",", 
       RowBox[{"state", "[", 
        RowBox[{"[", "2", "]"}], "]"}], ",", 
       RowBox[{"state", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], "]"}]}], "h"]}]}]}], "Input",
 CellChangeTimes->{{3.763067595400321*^9, 3.763067616008499*^9}, {
   3.7630677411286592`*^9, 3.7630677415446825`*^9}, {3.763067773208495*^9, 
   3.7630678158979383`*^9}, 3.763067864072695*^9, {3.7632523881367674`*^9, 
   3.7632523883637805`*^9}, {3.763252518108206*^9, 3.7632526205240664`*^9}}],

Cell[TextData[{
 "The Zeeman shift of a transition is therefore \[CapitalDelta]f ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"-", 
      SubscriptBox["\[Mu]", "B"]}], 
     RowBox[{"B", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", 
         RowBox[{"j", ",", "e"}]], 
        SubscriptBox["g", 
         RowBox[{"j", ",", "e"}]]}], "-", 
       RowBox[{
        SubscriptBox["m", 
         RowBox[{"j", ",", "g"}]], 
        SubscriptBox["g", 
         RowBox[{"j", ",", "g"}]]}]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", where the second subscript refers to the excited and ground state, \
respectively.  So the frequency shift between two states is"
}], "Text",
 CellChangeTimes->{{3.763067887910059*^9, 3.763067992662054*^9}, {
  3.763250248534325*^9, 3.7632502539586353`*^9}, {3.763250979031129*^9, 
  3.763250980631222*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalDelta]f", "[", 
    RowBox[{"B_", ",", "excited_", ",", "ground_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"\[CapitalDelta]Uz", "[", 
     RowBox[{"B", ",", "excited"}], "]"}], "-", 
    RowBox[{"\[CapitalDelta]Uz", "[", 
     RowBox[{"B", ",", "ground"}], "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.763250258863917*^9, 3.7632503570345345`*^9}, {
